{
  "name": "speedcontrol",
  "version": "0.1.0",
  "description": "speedcontrol",
  "main": "./src/server/index.js",
  "scripts": {
    "clean": "gulp clean",
    "lint": "gulp scripts:eslint",
    "flow-check": "flow check",
    "flow-coverage": "flow-coverage-report -i 'src/app/**/*.js' -i 'src/app/**/*.jsx' -t html -t json",
    "build": "gulp",
    "build:dist": "gulp dist --env production",
    "bundle-server": "nexe -n speedcontrol-front ./src/server/index.js",
    "test": "NODE_ENV=test jest",
    "watch": "gulp watch",
    "server": "node . --server.port=3000",
    "server:watch": "NODE_ENV=development nodemon -w src/server src/server/index.js -- --server.port=3000 --assets.maxAge=0 --api.baseUrl=http://localhost:8080/api"
  },
  "license": "ISC",
  "aliasify": {
    "replacements": {
      "@root/(.*)": "./src/app/$1"
    }
  },
  "browser": {
    "foundation-sites": "./node_modules/foundation-sites/dist/js/foundation.js"
  },
  "jest": {
    "moduleNameMapper": {
      "@root/(.*)": "<rootDir>/src/app/$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/.history"
    ],
    "setupTestFrameworkScriptFile": "./src/app/test.setup.js"
  },
  "dependencies": {
    "@ekz/async-data": "^0.4.2",
    "@ekz/formix": "^0.5.0-rc.4",
    "@ekz/option": "^1.2.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.3",
    "classnames": "^2.2.5",
    "cleave.js": "^1.4.7",
    "ejs": "^2.5.5",
    "foundation-sites": "^6.5.3",
    "immutable": "4.0.0-rc.12",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^3.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-helmet": "^4.0.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.5",
    "luxon": "^1.11.4",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.13",
    "motion-ui": "^2.0.3",
    "ramda": "^0.26.1",
    "rc": "^1.1.6",
    "react": "^16.8.3",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.8.3",
    "react-modal": "~3.8.1",
    "react-router-dom": "^4.2.2",
    "react-select": "^2.4.1",
    "react-table": "^6.9.2",
    "react-tippy": "^1.2.2",
    "scriptjs": "^2.5.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "aliasify": "^2.1.0",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.3",
    "browserify": "^16.2.3",
    "chalk": "^2.4.2",
    "del": "^4.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.92.0",
    "flow-coverage-report": "^0.6.1",
    "flow-typed": "^2.2.3",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-flatten": "^0.4.0",
    "gulp-hash": "^4.1.0",
    "gulp-if": "^2.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-tap": "^1.0.1",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.7",
    "jest": "^24.1.0",
    "merge-stream": "^1.0.0",
    "minimist": "^1.2.0",
    "modify-filename": "^1.1.0",
    "nexe": "^2.0.0-rc.34",
    "node-sass-import-once": "^1.2.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "require-dir": "^1.0.0",
    "run-sequence": "^2.0.0",
    "scssify": "^3.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
