[ignore]
<PROJECT_ROOT>/dist/.*
<PROJECT_ROOT>/build-tasks/.*
<PROJECT_ROOT>/src/server/.*
<PROJECT_ROOT>/target/.*
<PROJECT_ROOT>/project/.*
<PROJECT_ROOT>/bin/.*
<PROJECT_ROOT>/node_modules/module-deps/.*
<PROJECT_ROOT>/node_modules/config-chain/.*
<PROJECT_ROOT>/\.history/.*

[include]
flow-typed/

[libs]

[options]
esproposal.decorators=ignore
esproposal.export_star_as=enable
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnoreError
suppress_comment= \\(.\\|\n\\)*\\$FlowExpectError
include_warnings=true
munge_underscores=true
module.use_strict=true
module.name_mapper='^\@root\/\(.*\)$' -> '<PROJECT_ROOT>/src/app/\1'

[lints]
all=warn
unsafe-getters-setters=off

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
